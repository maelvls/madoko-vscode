{
    "name": "madoko",
    "displayName": "madoko",
    "description": "Syntax support for Madoko language",
    "version": "0.0.1",
    "publisher": "maelvalais",
    "repository": "https://github.com/maelvalais/madoko-vscode",
    "engines": {
        "vscode": "^1.10.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "madoko",
            "aliases": ["Madoko", "madoko"],
            "extensions": [".mdk",".madoko"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "madoko",
            "scopeName": "source.madoko",
            "path": "./syntaxes/madoko.tmLanguage"
        }]
    }
}